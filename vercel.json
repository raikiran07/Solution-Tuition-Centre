{
  "rewrites": [
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/programs",
      "destination": "/programs.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|jpg|jpeg|png|gif|svg|ico|webp|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
